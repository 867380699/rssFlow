{
	"appId": "com.ll.rssflow",
	"appName": "RssFlow",
	"webDir": "dist",
	"ios": {
		"limitsNavigationsToAppBoundDomains": true
	},
	"server": {
		"androidScheme": "http",
		"hostname": "localhost",
		"cleartext": true
	},
	"plugins": {
		"CapacitorHttp": {
			"enabled": true
		},
		"SplashScreen": {
			"launchAutoHide": false
		}
	},
	"packageClassList": [
		"SafeAreaPlugin",
		"AppPlugin",
		"FilesystemPlugin",
		"InAppBrowserPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"CDVPlugin"
	]
}
