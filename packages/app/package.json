{
  "name": "rss-flow-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "$npm_execpath build;vite preview",
    "lint": "eslint --ext .ts,.vue --fix src",
    "test": "vitest run",
    "sync": "$npm_execpath build;cap sync;cap sync @capacitor-community/electron",
    "ios": "cap open ios",
    "android": "cap open android",
    "electron": "cap open @capacitor-community/electron",
    "run:android": "adb devices | grep -w 'device' > /dev/null && $npm_execpath build && cap sync && cd android && ./gradlew installDebug --no-daemon && adb shell am start -n com.ll.rssflow/.MainActivity || echo 'No device found'"
  },
