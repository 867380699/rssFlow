{
  "editor.tabSize": 2,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "search.exclude": {
    "android/**": true,
    "**/pnpm-lock.yaml": true
  },
  "i18n-ally.localesPaths": [
    "src/locales"
  ],
  "i18n-ally.keystyle": "nested",
  "cSpell.words": [
    "composables",
    "dexie",
    "dompurify",
    "execpath",
    "intlify",
    "ionicons",
    "pinia",
    "precache",
    "precaching",
    "referrerpolicy",
    "scroller",
    "tailwindcss",
    "unplugin",
    "vite",
    "vitejs",
    "vueuse"
  ]
}